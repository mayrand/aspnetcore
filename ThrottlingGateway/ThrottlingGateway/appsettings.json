{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Methods": [
    {
      "Name": "GET",
      "Enabled": true,
      "Level": "high",
      "LowThrottleTime": 500,
      "HighThrottleTime": 1000
    },
    {
      "Name": "POST",
      "Enabled": true,
      "Level": "low",
      "LowThrottleTime": 500,
      "HighThrottleTime": 3000
    }
  ],
  "ThrottlingInfoUrl": "http://localhost:62146/api/LoadInfo/LoadInfo",
  "FunctionLevelCheckInterval": 500,
  "IdleCacheTime": 5000,
  "BusyCacheTime": 10000,
  "LogRequestResponse":  true,
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 62146
        }
      ],
      "UpstreamPathTemplate": "/{url}",
      "UpstreamHttpMethod": [ "Get", "Put", "Post", "Delete" ]
    }
  ]
}
