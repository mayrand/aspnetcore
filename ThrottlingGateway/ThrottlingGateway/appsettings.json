{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Methods": [
    {
      "Name": "GET",
      "Enabled": true,
      "Level": "high",
      "LowThrottleTime": 50,
      "HighThrottleTime": 300
    },
    {
      "Name": "POST",
      "Enabled": true,
      "Level": "low",
      "LowThrottleTime": 50,
      "HighThrottleTime": 300
    },
    {
      "Name": "PUT",
      "Enabled": true,
      "Level": "low",
      "LowThrottleTime": 50,
      "HighThrottleTime": 300
    },
    {
      "Name": "DELETE",
      "Enabled": false,
      "Level": "low",
      "LowThrottleTime": 50,
      "HighThrottleTime": 300
    }
  ],
  "ThrottlingInfoUrl": "http://localhost:62146/api/LoadInfo/LoadInfo",
  "FunctionLevelCheckInterval": 500,
  "IdleCacheTime": 500,
  "BusyCacheTime": 1000,
  "LogRequestResponse": true,
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 62146
        }
      ],
      "UpstreamPathTemplate": "/{url}",
      "UpstreamHttpMethod": [ "Get", "Put", "Post", "Delete" ]
    }
  ]
}
